  "require-dev": {
    "fabpot/php-cs-fixer": "1.*",
    "mockery/mockery": "dev-master",
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
